prefix=@prefix@
exec_prefix=@exec_prefix@
libexecdir=@libexecdir@
SED=@SED@

GARLIC=${prefix}/lib/garlic
GREP=grep

PATH=@libexecdir@:${PATH}
export PATH

if test $# = 0; then
  dist1
  exit 0
fi

LIBS="${GARLIC}/libgarlic.a ${GARLIC}/libgnat.a"

eval dist1 -I${GARLIC} -L${GARLIC} "$@" -largs ${LIBS}
