	@if [ -f $(GNATSRC)/sdefault.adb ]; then \
	  echo Copying sdefault.adb; \
	  cp $(GNATSRC)/sdefault.adb .; else \
	  echo Generating sdefault.adb; \
	  $(SHELL) $(srcdir)/config.sdefault; fi
