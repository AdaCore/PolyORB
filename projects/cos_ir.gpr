with "common", "src", "src_setup", "src_giop_diop";
limited with "src_corba";

project cos_ir is

   Dir := "../cos/ir";
   for Source_Dirs use (Dir);
   for Object_Dir use Dir;
   
   for Library_Name use "polyorb-corba-cos-ir-impl";
   for Library_Dir use Dir & "/" & ".libs";
   for Library_Kind use "static";

   package Compiler is

      for Default_Switches ("Ada")
         use Common.Compiler.Common_Ada_Compiler_Switches;

   end Compiler;

   for Locally_Removed_Files use ("po_ir.adb");

