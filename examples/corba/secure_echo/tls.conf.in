[security_manager]
own_credentials=tls_credentials
establish_trust_in_target_required=true

[access_points]
diop=disable

[tls_credentials]
transport_credentials_type=tls
tls.method=tls1
tls.ciphers=ALL
tls.verify_peer=true
tls.verify_fail_if_no_peer_certificate=true
tls.private_key_file=polyorb.key
tls.certificate_file=polyorb.crt
tls.certificate_authority_file=root.crt

[tlsiop]

[My_POA]
transport_mechanism=tlsiop
