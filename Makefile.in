# Common build rule for all compile-time tools #
################################################

COMPILERS_GNATMAKE_FLAGS := @STYLE_SWITCH@ -gnatwa@WARNINGS_MODE@ ${GNATMAKE_FLAGS}

build-gnatprfh \
\
build-testparser \
build-testlexer \
build-testgen \
