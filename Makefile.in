################################################################

# Installation

prefix=@prefix@

.PHONY: install
install: all
	@echo "Install prefix: ${prefix}"
	@echo "   Sources dir: ${top_srcdir}"
	@echo "     Build dir: ${top_builddir}"
	${INSTALL} -d ${prefix}/bin
	${RM} -fr "${prefix}/lib/gnat/polyorb"
	${INSTALL} -d ${prefix}/lib/gnat/polyorb
	${INSTALL} -d ${prefix}/lib/polyorb
	${RM} -fr "${prefix}/include/polyorb"
	${INSTALL} -d ${prefix}/include/polyorb
	for comp in ${COMPILER_EXES}; do \
	  ${INSTALL} ${top_builddir}/compilers/$${comp}${COMPILER_EXE_SUFFIX} ${prefix}/bin; \
	  # Special case for iac: also symlink as idlac for backwards \
	  # compatibility \
	  if [ `basename $${comp}` = iac ]; then \
	    cd ${prefix}/bin && ${RM} -f idlac && ${LN_S} iac idlac; \
          fi; \
	done
	${INSTALL} ${top_builddir}/tools/po_cos_naming/po_cos_naming${TOOL_EXE_SUFFIX} ${prefix}/bin
	${INSTALL} ${top_builddir}/tools/po_cos_naming/po_cos_naming_shell${TOOL_EXE_SUFFIX} ${prefix}/bin
	${INSTALL} ${top_builddir}/tools/po_cos_naming/ir_ab_names${TOOL_EXE_SUFFIX} ${prefix}/bin
	${INSTALL} ${top_builddir}/tools/po_ir/po_ir${TOOL_EXE_SUFFIX} ${prefix}/bin
	${INSTALL} ${top_builddir}/tools/po_catref/po_catref${TOOL_EXE_SUFFIX} ${prefix}/bin
	${INSTALL} ${top_builddir}/tools/po_createref/po_createref${TOOL_EXE_SUFFIX} ${prefix}/bin
	${INSTALL} ${top_builddir}/tools/po_names/po_names${TOOL_EXE_SUFFIX} ${prefix}/bin
	${INSTALL} ${top_builddir}/tools/po_dumpir/po_dumpir${TOOL_EXE_SUFFIX} ${prefix}/bin
	${INSTALL} ${top_builddir}/polyorb-config ${prefix}/bin
	ls ${top_builddir}/lib/*.a | ${XARGS_I} ${INSTALL} -m 444 {} ${prefix}/lib
	ls ${top_srcdir}/projects-distrib/*.gpr | ${XARGS_I} ${INSTALL} -m 444 {} ${prefix}/lib/gnat
	ls ${top_builddir}/projects-distrib/*.gpr | ${XARGS_I} ${INSTALL} -m 444 {} ${prefix}/lib/gnat
	ls ${top_srcdir}/projects-distrib/polyorb/*.gpr | ${XARGS_I} ${INSTALL} -m 444 {} ${prefix}/lib/gnat/polyorb
	ls ${top_builddir}/projects-distrib/polyorb/*.gpr | ${XARGS_I} ${INSTALL} -m 444 {} ${prefix}/lib/gnat/polyorb
	(find \
	  ${top_builddir}/src \
	  ${top_builddir}/cos \
	  ${top_builddir}/idls \
	  ${top_srcdir}/src \
	  ${top_srcdir}/cos \
	  ${top_srcdir}/idls \
	  -name '*.ads' -o -name '*.adb' -o -name '*.idl') | \
	${XARGS_I} ${INSTALL} -m 444 {} ${prefix}/include/polyorb
	(find \
	  ${top_builddir}/ali \
	  -name '*.ali' \
	  -a ! -name 'polyorb-dsa_p-partitions.ali' \
	  -a ! -name 'polyorb-partition_elaboration.ali') | \
	${XARGS_I} ${INSTALL} -m 444 {} ${prefix}/lib/polyorb
	# Install documentation only if it has been built:
	if [ -r docs/polyorb_ug.html ]; \
	then \
	  (cd docs && ${MAKE} install prefix="${prefix}"); \
	fi

# polyorb-dsa_p-partitions.ali is a special case above.
# It is never installed, so that this unit is recompiled for each application
# by gnatdist. This is necessary because two compilations must occur:
# receiving stubs for the main partition, and calling stubs for others.
#
# Note that any change of the location where DSA application personality
# files are installed (relative to ${prefix}) must be reflected in
# gnatdist.

