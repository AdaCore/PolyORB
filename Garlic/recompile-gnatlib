#!/bin/sh
# $Id$

# Find out where the GNAT library is located
prefix=`gnatls -I- -v|tail -2|head -1 | sed -e 's,lib/$,include/,g'`

# Command to compile
cmd="gcc -c -gnatg -gnatpn -O0 $prefix"

# Compile all library units depending on Stream_Attributes
${cmd}a-caldel.adb
${cmd}a-calend.adb
${cmd}a-dynpri.adb
${cmd}a-filico.adb
${cmd}a-finali.adb
${cmd}a-flteio.ads
${cmd}a-fwteio.ads
${cmd}a-inteio.ads
