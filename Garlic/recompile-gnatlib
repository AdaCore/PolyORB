${cmd}a-reatim.adb
${cmd}a-retide.adb
${cmd}a-sfteio.ads
${cmd}a-sfwtio.ads
${cmd}a-siteio.ads
${cmd}a-siwtio.ads
${cmd}a-ssicst.adb
${cmd}a-ssitio.ads
${cmd}a-ssiwti.ads
${cmd}a-strunb.adb
