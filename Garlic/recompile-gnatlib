${cmd}a-intnam.ads
${cmd}a-iwteio.ads
${cmd}a-lfteio.ads
${cmd}a-lfwtio.ads
${cmd}a-liteio.ads
${cmd}a-liwtio.ads
${cmd}a-llftio.ads
${cmd}a-llfwti.ads
${cmd}a-llitio.ads
